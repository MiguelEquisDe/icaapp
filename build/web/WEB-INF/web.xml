<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>BeensCasinos</servlet-name>
        <servlet-class>beens.BeensCasinos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorCasino</servlet-name>
        <servlet-class>controlador.ControladorCasino</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorPruebas</servlet-name>
        <servlet-class>controlador.ControladorPruebas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorImgP</servlet-name>
        <servlet-class>controlador.ControladorImgP</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Sandbox</servlet-name>
        <servlet-class>sandbox.Sandbox</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PushSocket</servlet-name>
        <servlet-class>Mail.PushSocket</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmailSenderService</servlet-name>
        <servlet-class>controlador.EmailSenderService</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BeensCasinos</servlet-name>
        <url-pattern>/BeensCasinos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorCasino</servlet-name>
        <url-pattern>/ControladorCasino</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorPruebas</servlet-name>
        <url-pattern>/ControladorPruebas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorImgP</servlet-name>
        <url-pattern>/ControladorImgP</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Sandbox</servlet-name>
        <url-pattern>/Sandbox</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PushSocket</servlet-name>
        <url-pattern>/PushSocket</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmailSenderService</servlet-name>
        <url-pattern>/EmailSenderService</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
